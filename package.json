{
  "name": "yaiza",
  "version": "1.0.2",
  "description": "Yaiza",
  "main": "index.js",
  "engines": {
    "node": "6.11.1"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --port 8888 --colors --inline --content-base public/ --history-api-fallback",
    "start:prod": "npm run build",
    "build:client": "webpack -p --progress --colors",
    "build:server": "webpack --colors --progress --config webpack.server.config.js",
    "build": "npm run start:prod",
    "build:sass": "node-sass styles/index.scss public/index.css",
    "postbuild": "npm run build:sass"
  },
  "resolutions": {
    "unidragger": "2.2.3"
  },
  "repository": {
    "type": "git"
  },
  "author": "Owen Nicol",
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "css-loader": "^0.25.0",
    "exenv": "^1.2.1",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "http-server": "^0.9.0",
    "if-env": "^1.0.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "mobile-detect": "^1.3.5",
    "node-sass": "^3.13.0",
    "prismic.io": "^3.3.0",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.4.2",
    "react-flickity-component": "^0.1.0",
    "react-helmet": "^5.2.0",
    "react-html5video": "^1.4.0",
    "react-reveal": "^0.1.1",
    "react-router": "^3.0.0",
    "react-slick": "^0.14.5",
    "sass-loader": "4.0.2",
    "slick-carousel": "^1.6.0",
    "style-loader": "^0.13.1",
    "superagent": "^3.8.3",
    "swiper": "^3.4.1",
    "unidragger": "2.2.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "1.16.2"
  },
  "heroku-run-build-script": true
}
