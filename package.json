{
  "name": "yaiza",
  "version": "0.0.3",
  "description": "Yaiza",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --colors --inline --content-base public/ --history-api-fallback",
    "start:prod": "npm run build && node server.bundle.js",
    "build:client": "webpack",
    "build:server": "webpack --colors --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server",
    "build:sass": "node-sass styles/index.scss public/index.css",
    "postbuild": "npm run build:sass",
    "postinstall": "npm run start:prod"
  },
  "repository": {
    "type": "git"
  },
  "author": "Owen Nicol",
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-cli": "^6.14.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "http-server": "^0.9.0",
    "if-env": "^1.0.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.13.0",
    "prismic.io": "^3.3.0",
    "react": "^15.4.0",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.4.0",
    "react-flickity-component": "^0.1.0",
    "react-router": "^3.0.0",
    "sass-loader": "4.0.2",
    "style-loader": "^0.13.1",
    "superagent": "^2.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "1.16.2"
  }
}
